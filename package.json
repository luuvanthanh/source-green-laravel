{
  "name": "boilerplate-umi",
  "version": "0.0.1",
  "private": true,
  "author": "PhuTV",
  "email": "phutv@greenglobal.vn",
  "license": "UMI",
  "description": "An out-of-box UI solution for enterprise applications",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start:dev",
    "fetch:blocks": "pro fetch-blocks && npm run prettier",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "npm run lint:js",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx ./src",
    "lint:style": "stylelint --fix \"src/**/*.scss\" --syntax scss",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start": "umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "pretest": "node ./tests/beforeTest",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/*.scss": "stylelint --syntax scss",
    "src/*.{js,jsx,tsx,ts,scss,md,json}": [
      "git add"
    ],
    "src/*.{js,jsx}": "npm run lint-staged:js",
    "src/*.{js,ts,tsx}": "npm run lint-staged:js"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@casl/ability": "^5.2.2",
    "@casl/react": "^2.2.2",
    "@fullcalendar/core": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/list": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@fullcalendar/resource-timegrid": "^5.5.0",
    "@fullcalendar/timegrid": "^5.5.1",
    "@umijs/route-utils": "^1.0.33",
    "animate.css": "^4.1.1",
    "antd": "^4.13.1",
    "array-move": "^3.0.1",
    "babel-eslint": "^10.1.0",
    "bootstrap-css-only": "^4.4.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "husky": "^4.3.0",
    "immutability-helper": "^3.1.1",
    "json-api-normalizer": "^1.0.4",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.11",
    "moment": "^2.25.3",
    "node-sass": "^4.14.1",
    "nprogress": "^0.2.0",
    "omit.js": "^2.0.2",
    "prop-types": "^15.7.2",
    "qs": "^6.9.4",
    "rc-drawer": "^4.1.0",
    "react": "^17.0.1",
    "react-c3js": "^0.1.20",
    "react-custom-scrollbars": "^4.2.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^17.0.0",
    "react-helmet": "^6.1.0",
    "react-infinite-scroller": "^1.2.4",
    "react-leaflet": "^3.1.0",
    "react-quill": "^1.3.5",
    "react-slick": "^0.28.1",
    "react-sortable-hoc": "^1.11.0",
    "redux-object": "^1.0.0",
    "rrule": "^2.6.8",
    "sass-loader": "^10.0.5",
    "slick-carousel": "^1.8.1",
    "store": "^2.0.12",
    "umi": "^3.2.14",
    "umi-request": "^1.0.8",
    "universal-cookie": "^4.0.4",
    "use-merge-value": "^1.0.1",
    "validator": "^13.5.2",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "@types/react-helmet": "^6.1.0",
    "@umijs/fabric": "^2.3.0",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "^1.4.8",
    "cross-env": "^7.0.0",
    "eslint": "^7.1.0",
    "express": "^4.17.1",
    "gh-pages": "^3.0.0",
    "lint-staged": "^10.0.0",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.0.1",
    "puppeteer-core": "^5.0.0",
    "stylelint": "^13.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.scss",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
