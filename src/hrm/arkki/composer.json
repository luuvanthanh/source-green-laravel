{
    "name": "kun391/students",
    "description": "A Package for management students with jsonapi specification",
    "keywords": [
        "kun391",
        "students"
    ],
    "homepage": "https://github.com/kun391/students",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kun",
            "email": "nguyentruongthanh.dn@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kun391\\Clover\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kun391\\Clover\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kun391\\Clover\\CloverServiceProvider"
            ],
            "aliases": {
                "Clover": "Kun391\\Clover\\CloverFacade"
            }
        }
    }
}