{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "alhimik1986/php-excel-templator": "^1.0",
        "berkayk/onesignal-laravel": "^1.0",
        "cloudcreativity/laravel-json-api": "^2.0",
        "coraxster/flysystem-aws-s3-v3-minio": "^1.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "league/fractal": "^0.19.2",
        "orangehill/iseed": "^2.6",
        "prettus/l5-repository": "^2.6",
        "spatie/laravel-medialibrary": "^8.0.0",
        "staudenmeir/eloquent-json-relations": "^1.1",
        "webpatser/laravel-uuid": "^4.0",
        "zerodahero/laravel-workflow": "^3.1"
    },
    "require-dev": {
        "cloudcreativity/json-api-testing": "^2.1",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5",
        "spatie/laravel-cronless-schedule": "^1.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Packages\\": "packages/",
            "GGPHP\\Core\\": "packages/core/src/",
            "GGPHP\\Users\\": "packages/users/src/",
            "GGPHP\\ShiftSchedule\\": "packages/shift-schedule/src/",
            "GGPHP\\FingerprintTimekeeper\\": "packages/fingerprint-timekeeper/",
            "GGPHP\\Timekeeping\\": "packages/timekeeping/src/",
            "GGPHP\\Fingerprint\\": "packages/fingerprint/src/",
            "GGPHP\\Absent\\": "packages/absent/src/",
            "GGPHP\\LateEarly\\": "packages/late-early/src/",
            "GGPHP\\AddSubTime\\": "packages/add-sub-time/src/",
            "GGPHP\\Config\\": "packages/config/src/",
            "GGPHP\\RevokeShift\\": "packages/revoke-shift/src/",
            "GGPHP\\WorkDeclaration\\": "packages/work-declaration/src/",
            "GGPHP\\WorkHour\\": "packages/work-hour/src/",
            "GGPHP\\Category\\": "packages/category/src/",
            "GGPHP\\Profile\\": "packages/profile/src/",
            "GGPHP\\Transfer\\": "packages/transfer/src/",
            "ZK\\": "packages/zk/src"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}