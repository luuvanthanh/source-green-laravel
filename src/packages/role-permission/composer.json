{
    "name": "kun391/users",
    "description": "A Package for management users with jsonapi specification",
    "keywords": [
        "kun391",
        "users"
    ],
    "homepage": "https://github.com/kun391/users",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "PHP Group",
            "email": "php@greenglobal.vn",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0",
        "spatie/laravel-permission": "^3.11"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kun391\\Users\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kun391\\Users\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kun391\\Users\\UsersServiceProvider"
            ],
            "aliases": {
                "Users": "Kun391\\Users\\UsersFacade"
            }
        }
    }
}
